{
  "name": "@anyswap/client",
  "version": "0.1.0",
  "description": "TypeScript client library for AnySwap protocol",
  "main": "dist/index.js",
  "module": "lib/index.ts",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.ts",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "ts-mocha -p tsconfig.json tests/**/*.test.ts",
    "test:watch": "ts-mocha -p tsconfig.json --watch tests/**/*.test.ts"
  },
  "keywords": [
    "solana",
    "anchor",
    "defi",
    "swap",
    "amm",
    "balancer",
    "anyswap"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/K-M-official/anyswap.git",
    "directory": "app"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.32.1",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.4"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "ts-mocha": "^10.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0"
  }
}

